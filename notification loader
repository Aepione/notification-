function CheckNotifGuiP()
pcall(function()
if not game.CoreGui:FindFirstChild("Notification | Cz8pLe-dJem1m-l1mdka-sxi53d") then
local NotificationCz8pLedJem1ml1mdkasxi53d = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
